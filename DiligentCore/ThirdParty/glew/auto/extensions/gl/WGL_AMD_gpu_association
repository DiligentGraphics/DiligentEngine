	WGL_GPU_CLOCK_AMD 0x21A4
	WGL_GPU_NUM_PIPES_AMD 0x21A5
	WGL_GPU_NUM_SIMD_AMD 0x21A6
	WGL_GPU_NUM_RB_AMD 0x21A7
	WGL_GPU_NUM_SPI_AMD 0x21A8
