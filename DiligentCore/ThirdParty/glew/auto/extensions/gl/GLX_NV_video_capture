	GLXVideoCaptureDeviceNV * glXEnumerateVideoCaptureDevicesNV (Display* dpy, int screen, int *nelements)
	void glXLockVideoCaptureDeviceNV (Display* dpy, GLXVideoCaptureDeviceNV device)
