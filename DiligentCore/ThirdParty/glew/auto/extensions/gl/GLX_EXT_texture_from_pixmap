	GLX_TEXTURE_1D_BIT_EXT 0x00000001
	GLX_TEXTURE_2D_BIT_EXT 0x00000002
	GLX_TEXTURE_RECTANGLE_BIT_EXT 0x00000004
	GLX_BIND_TO_TEXTURE_RGB_EXT 0x20D0
	GLX_BIND_TO_TEXTURE_RGBA_EXT 0x20D1
