    </collision>
    <inertial>
      <mass value="2"/>
      <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0" iyz="0.0" izz="0"/>
    </inertial>
  </link>
  <joint name="Base_pulley1" type="revolute">
    <axis xyz="0 0 1"/>
    <parent link="Base"/>
    <child link="pulley1"/>
    <limit effort="0" lower="1" upper="0" velocity="50"/>
    <origin rpy="-1.57075 1.57075 0" xyz="0 .35 .15"/>
  </joint>

